pipeline {
    agent any

parameters{
string{name: 'env', defaultValue: 'test',description:'env to compile'}
}

    stages {
        stage('compile') {
            steps {
                script{
                    echo "compile Hello World"
echo "compile in env: ${params.env}"
                }
            }
        }
       stage('test') {
            steps {
                script{
                    echo "test-Hello World"
                }
            }
        }
         stage('package') {
            steps {
                script{
                    echo " package-Hello World"
                }
            }
        }
    }
}
